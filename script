			if v:IsA("BodyVelocity") or v:IsA("BodyPosition")or v:IsA("BodyAngularVelocity") or v:IsA("BodyForce") or v:IsA("BodyGyro") or v:IsA("BodyThrust") then
				v:Destroy()
				wait(0.1)
